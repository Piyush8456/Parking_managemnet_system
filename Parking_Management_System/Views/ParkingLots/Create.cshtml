@using Parking_Management_System.Enum_s
@model ParkingLots;
<div>
    <h2>Create Parking Lot</h2>

    <form asp-controller="ParkingLots" asp-action="Create">
        <div class="form-group">
            <label asp-for="Name"></label>
            <input asp-for="Name" />
            <span asp-validation-for="Name" class="text-denger"></span>
        </div>
        <div class="form-group">
            <label asp-for="TotalCapacity"></label>
            <input asp-for="TotalCapacity" />
            <span asp-validation-for="TotalCapacity" class="text-denger"></span>
        </div>
        <div class="form-group">
            <label asp-for="AvailableSpaces"></label>
            <input asp-for="AvailableSpaces" />
            <span asp-validation-for="AvailableSpaces" class="text-denger"></span>
        </div>
        <div class="form-group">
            <select name="selectedOption">
                <option value="" disabled selected>Select an option</option>
                @foreach (var item in Model)
                {
                    <option value="@item">@item</option>
                }
            </select>
        </div>

    </form>
</div>

